{
  "name": "prisma-json-types-generator",
  "description": "Changes JsonValues to your custom typescript type",
  "version": "1.0.1",
  "main": "dist/generator.js",
  "bin": "./dist/bin.js",
  "license": "MIT",
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "npx tsc -w",
    "build": "npx tsc",
    "prepack": "yarn build",
    "test": "jest",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@prisma/generator-helper": "4.8.1",
    "@prisma/sdk": "^4.0.0"
  },
  "devDependencies": {
    "@arthurfiorette/prettier-config": "^1.0.7",
    "@types/node": "18.11.18",
    "@types/prettier": "2.7.2",
    "prettier": "2.8.1",
    "prisma": "4.8.1",
    "typescript": "4.9.4"
  },
  "keywords": [
    "prisma",
    "prisma2",
    "generator"
  ],
  "author": "Arthur Fiorette <arthur.fiorette@gmail.com>"
}
